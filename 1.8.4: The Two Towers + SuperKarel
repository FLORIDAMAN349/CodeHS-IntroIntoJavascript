//starts code
function start(){
    move();
    turnLeft();
    makePancakes();
    move();
    move();
    turnLeft();
    makePancakes();
    turnLeft();
    move();
    move();
    move();
    turnRight();
}
//makes pancakes
function makePancakes() {
     putBall();
     move();
     putBall();
     move();
     putBall();
     turnAround();
     move();
     move();
     turnLeft();
}
